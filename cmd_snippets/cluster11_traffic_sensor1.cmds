# Sensor s_11_1 on edge 10-19
echo "11" > /etc/cluster_id
echo "s_11_1" > /etc/sensor_id
echo "EDGE=10-19" > /etc/edge_info
echo "MANUFACTURER=OkayDevices" > /etc/sensor_profile
