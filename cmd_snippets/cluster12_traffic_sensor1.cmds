# Sensor s_12_1 on edge 2-11
echo "12" > /etc/cluster_id
echo "s_12_1" > /etc/sensor_id
echo "EDGE=2-11" > /etc/edge_info
echo "MANUFACTURER=LegacySystems" > /etc/sensor_profile
echo "SOFTWARE_VERSION=v2.0.0-signed" >> /etc/sensor_profile
echo "IS_SIGNED=true" >> /etc/sensor_profile
echo "SOFTWARE_AGE_YEARS=1.5" >> /etc/sensor_profile
echo "DEVICE_AGE_YEARS=0.18" >> /etc/sensor_profile
echo "MAKE_NOISY=false" > /etc/sensor_config
echo "192.168.254.253" > /etc/traffic_server_ip
