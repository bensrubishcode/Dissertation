# Sensor s_6_1 on edge 2-10
echo "6" > /etc/cluster_id
echo "s_6_1" > /etc/sensor_id
echo "EDGE=2-10" > /etc/edge_info
echo "MANUFACTURER=ShadySensorsLtd" > /etc/sensor_profile
echo "SOFTWARE_VERSION=v1.0.0" >> /etc/sensor_profile
echo "IS_SIGNED=false" >> /etc/sensor_profile
echo "SOFTWARE_AGE_YEARS=4.0" >> /etc/sensor_profile
echo "DEVICE_AGE_YEARS=3.7" >> /etc/sensor_profile
echo "MAKE_NOISY=false" > /etc/sensor_config
echo "192.168.254.253" > /etc/traffic_server_ip
